<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="io.github.talelin.latticy.mapper.my.DeliveryMapper">

    <select id="searchDeliveryNoByOrderNo" resultType="java.lang.String">
        select delivery_no
        from delivery
        where order_no = #{orderNo}
    </select>

</mapper>